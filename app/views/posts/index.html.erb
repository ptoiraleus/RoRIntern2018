<div class="jumbotron">
<h1>All Posts</h1>
</div>

<div class="container">
  <div style="margin-bottom: 20px">
    <%= link_to 'New post', new_post_path, class: 'btn btn-primary'  %>
  </div>
  
  <div class="posts">
    <% @posts.each do |post| %>
      <%= render 'post', post: post %>
    <%end%>
  </div>
</div>
